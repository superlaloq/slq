{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "button-type": "off",
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-size-adjust",
          "-webkit-tap-highlight-color",
          "scroll-behavior",
          "text-decoration-skip-ink",
          "-webkit-overflow-scrolling",
          "overflow-anchor"
        ]
      }
    ]
  }
}